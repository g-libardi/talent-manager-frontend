{
  "dependencies": {
    "axios": "^1.7.7",
    "concurrently": "^9.0.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "http-proxy-middleware": "^3.0.3",
    "jwt-decode": "^4.0.0",
    "nodemon": "^3.1.7"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm --prefix ../app run devhost",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm --prefix ../app run build && cp -r ../app/build/* ./static/."
  }
}
